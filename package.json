		"axios": "^0.21.1",
		"class-transformer": "^0.3.1",
		"class-validator": "^0.12.2",
		"crypto-js": "^4.0.0",
		"form-data": "^4.0.0",
		"helmet": "^4.2.0",
		"lodash": "^4.17.21",
